

_name=libkomparediff2

source=(http://download.kde.org/stable/$version/src/${_name}-$version.tar.xz)
	
build() {
cd ${_name}-$version
mkdir build
cd build
cmake -DCMAKE_INSTALL_PREFIX=/usr \
      -DSYSCONF_INSTALL_DIR=/etc/kde         \
      -DCMAKE_BUILD_TYPE=Release         \
      -Wno-dev .. 
make
make DESTDIR=$PKG install
}


